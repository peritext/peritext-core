{
    "type": "object",
    "id": "/images-group.json",
    "title": "Images group",
    "description": "A group of images to be displayed as a series",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "images": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "imageDataset": {
                        "type": "string",
                        "description": "dataset id (uuid v4) for the image resource",
                        "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
                    }
                }
            }
        }
    }
}