{
    "type": "object",
    "id": "/embed.json",
    "title": "Embed",
    "description": "Embed code resources' data model",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "thumbnailDataset": {
            "type": "string",
            "description": "static thumbnail dataset id (uuid v4) for the resource",
            "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
        },
        "html": {
            "type": "string",
            "description": "html code to embed"
        }
    }
}